[common]
pkgname="${pkgprefix}gcc${abbrev_ver}"
name="gcc"
pkgver="$pkgver"
shortdesc="GNU compiler suite"

[base]
subsys=sw.base
files(-,root,sys)
${iprefix}/$_bindir
${iprefix}/$_libdir
${iprefix}/$_includedir
${iprefix}/libexec

[man]
subsys=man.man
files(-,root,sys)
${iprefix}/$_mandir

[info]
subsys=man.info
files(-,root,sys)
${iprefix}/$_infodir

[lib]
subsys=sw.lib
files(-,root,sys)
$_libdir/*.so.*

