[common]
pkgname="$pkgprefix""$topdir"
name="$topdir"
pkgver="$pkgver"
pkgvendor="http://http://www.freedesktop.org/software/fontconfig/releases/"
shortdesc="X Render extension library"

[lib]
subsys=sw.lib
files(-,root,sys)
$_libdir/*.so.*
$_docdir

[devel]
subsys=sw.dev
files(-,root,sys)
$_includedir
$_libdir/*.so
$_libdir/*.a
$_libdir/pkgconfig/*.pc

