[common]
pkgname="$pkgprefix""xrender"
name="$topdir"
pkgver="$pkgver"
pkgvendor="http://http://www.freedesktop.org/software/fontconfig/releases/"
shortdesc="Client library for the X Rendering Extension protocol"

[lib]
subsys=sw.lib
files(-,root,sys)
$_libdir/*.so.*
$_docdir

[devel]
subsys=sw.dev
files(-,root,sys)
$_includedir
$_libdir/*.so
$_libdir/*.a
$_libdir/pkgconfig/*.pc

