[common]
pkgname="$pkgprefix""$topdir"
name="$topdir"
pkgver="$pkgver"
pkgvendor="http://www.musepack.net"
shortdesc="Portable Musepack decoder library"

[lib]
subsys=sw.lib
files(-,root,sys)
$_libdir/*.so.*
$_sharedir

[devel]
subsys=sw.dev
files(-,root,sys)
$_includedir
$_libdir/*.so
$_libdir/*.a

