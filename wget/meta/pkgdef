[common]
pkgname="${pkgprefix}${topdir}"
name="$topdir"
pkgver="$pkgver"
pkgvendor="http://directory.fsf.org/wget.html"
shortdesc="A utility for retrieving files using the HTTP or FTP protocols"

[base]
subsys=sw.base
files(-,root,sys)
$_bindir
$_sharedir
$_sysconfdir

[man]
subsys=man.man
files(-,root,sys)
$_mandir/?a?1/wget.1*

[info]
subsys=man.info
files(-,root,sys)
$_infodir

